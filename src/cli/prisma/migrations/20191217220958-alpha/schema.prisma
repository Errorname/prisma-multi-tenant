datasource management_sqlite {
  provider = "sqlite"
  url = "***"
  enabled  = env("PMT_MANAGEMENT_PROVIDER_SQLITE")
}

generator photon {
  provider = "photonjs"
  output   = env("PMT_OUTPUT")
}

model Tenant {
  id       String @default(cuid()) @id
  name     String @unique
  provider String
  url      String
}